    "flowbite-react": "^0.7.2",
    "i18next": "^23.7.8",
    "i18next-browser-languagedetector": "^7.2.0",
    "json-server": "^0.17.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^13.5.0",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.19.0",
    "vit": "^0.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
